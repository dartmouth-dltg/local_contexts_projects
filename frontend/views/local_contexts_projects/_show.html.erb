
<section id="<%= section_id %>">
  <h3><%= I18n.t("plugins.local_contexts_projects._plural") %><a class="btn btn-sm btn-default ml-auto" id="fetch-local-contexts-data"><%= I18n.t('local_contexts_project.fetch_btn') %></a></h3>
  <div class="details">
    <div id="<%= section_id %>_local_contexts_projects">
      <div class="subrecord-form-dummy">
        <div class="subrecord-form-container">
          <div class="subrecord-form-fields">
            <div class="row">
              <div class="col-md-4 token-list">
                <% local_contexts_projects.each do | local_contexts_project | %>
                  <%= render_token :object => local_contexts_project,
                  :label => local_contexts_project["_resolved"]["display_string"],
                  :type => local_contexts_project["_resolved"]["jsonmodel_type"],
                  :uri => local_contexts_project["_resolved"]["uri"],
                  :icon_class => "local-contexts-project-id-" + local_contexts_project["_resolved"]["project_id"]%>
                <% end %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div id="local-contexts-data-holder" class=""></div>
  </div>
</section>
<%= javascript_tag "new LocalContexts();" %>
